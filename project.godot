; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Crypts of the Forgotten"
config/description="A top-down 2D multiplayer dungeon crawler built in Godot 4.4.1"
run/main_scene="res://scenes/MainMenu.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

NetworkManager="*res://scripts/NetworkManager.gd"
GameManager="*res://scripts/GameManager.gd"
ProgressionManager="*res://scripts/ProgressionManager.gd"
AudioManager="*res://scripts/AudioManager.gd"

[debug]

settings/stdout/print_fps=true
settings/stdout/verbose_stdout=true
settings/crash_handler/message="Game crashed! Check the logs for details."
settings/profiler/max_errors_per_second=1000
settings/profiler/max_warnings_per_second=1000

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/mode=2

[rendering]

driver/driver_name="opengl3"
driver/threads/thread_model=2
renderer/rendering_method="forward_plus"
textures/canvas_textures/default_texture_filter=1
2d/use_software_skinning=true
vulkan/rendering/back_end=0
vulkan/rendering/driver.allow_invalid_extensions=false

