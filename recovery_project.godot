config_version=5

[application]

config/name="Crypts of the Forgotten"
config/description="A top-down 2D multiplayer dungeon crawler built in Godot 4.4.1"
run/main_scene="res://minimal_test.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

NetworkManager="*res://scripts/NetworkManager.gd"
GameManager="*res://scripts/GameManager.gd"
ProgressionManager="*res://scripts/ProgressionManager.gd"
AudioManager="*res://scripts/AudioManager.gd"

[debug]

settings/stdout/print_fps=true
settings/stdout/verbose_stdout=true
settings/crash_handler/message="Game crashed! Check the logs for details."
settings/profiler/max_errors_per_second=1000
settings/profiler/max_warnings_per_second=1000

